{
    "compilerOptions": {
        "skipLibCheck": true,
        "target": "es5",
        "module": "es2020",
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "rootDir": ".",
        "outDir": "dist",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }